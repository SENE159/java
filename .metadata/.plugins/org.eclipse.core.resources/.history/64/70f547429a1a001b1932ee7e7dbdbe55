import java.util.ArrayList;
import java.util.Scanner;

public class Exer9Algo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
  Scanner clavier = new Scanner(System.in);
  System.out.println("Entrée un nombre pour la taille du tableau");
  int nombre = clavier.nextInt();
  int tab [] = new int [nombre];
  
  for (int i = 0; i < nombre+1; i++) 
  {
	  System.out.println("Entrée le nombre à la position " + i + " :");
		Scanner clav1 = new Scanner(System.in);
		tab[i] = clav1.nextInt(); 
	 	
}
  System.out.println("Entrée la valeur à ajouter dans le tableau");
  int valeur = clavier.nextInt();
  
  ArrayList<String> array = new ArrayList<String>();
  
  array.add("5, valeur");
	System.out.println(array.get(5));		
		}
	
}
		
		

				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
		
		
		
			


