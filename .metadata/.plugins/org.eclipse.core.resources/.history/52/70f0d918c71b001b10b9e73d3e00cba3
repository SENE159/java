
public class Testtabliv {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int tab [] = new int [3];
		
		for (int i = 0; i < tab.length; i++) {
			
			tab[i] = i + 1;
			
		}
		for (int i = 0; i < tab.length; i++) {
			
			System.out.println("la tab["+i+"] est égale à :" + tab[i]);
			
		}

	}

}
